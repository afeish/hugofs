# by set HUGO_TRACE_ENABLED to true, we enable the tracing and profiling
HUGO_TRACE_ENABLED=true
OTEL_TRACES_SAMPLER=parentbased_traceidratio
# control the sample frequency
OTEL_TRACES_SAMPLER_ARG=0.01
TRACING_BACKEND_URL=jaeger:4317
# provide http prefix needed
PYROSCOPE_URL=http://pyroscope:4040
TCP_PORT=40001
HUGO_TRANSPORT=tcp
HUGO_LOG_LEVEL=debug
HUGO_BLOCK_SIZE=512K
HUGO_CACHE_FILE_SIZE=4G
HUGO_OBJECT_PLATFORM=local
HUGO_OBJECT_PREFIX=dev
HUGO_OBJECT_ACCESS_KEY=
HUGO_OBJECT_SECURE_KEY=
HUGO_OBJECT_BUCKET_NAME=/tmp/s3

HUGO_ALLOWOTHER=true
